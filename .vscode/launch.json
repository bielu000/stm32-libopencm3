    "version": "0.2.0",
    "configurations": [
    {
        "name": "(gdb) Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "/home/pp/STM32/STM32_Template/src/utils/test/bin/Utils_tests",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ]
    },
    {
        "name": "Cortex Debug",
        "cwd": "${workspaceRoot}",
        "executable": "./build/bin/stm32-template",
        "request": "launch",
        "type": "cortex-debug",
        "servertype": "jlink",
        "device": "STM32F103C8",
        "showDevDebugOutput" : true
    }
    ]
}